<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Outer Ring -->
    <item>
        <shape android:shape="oval">
            <size android:width="91dp" android:height="91dp"/>
            <solid android:color="#004D40" /> <!-- hijau gelap -->
        </shape>
    </item>

    <!-- Inner Circle -->
    <item android:top="10dp" android:bottom="10dp" android:left="10dp" android:right="10dp">
        <shape android:shape="oval">
            <solid android:color="@color/hijau"/> <!-- hijau terang -->
        </shape>
    </item>

</layer-list>
